#include "stdio.h"
void main ()
{
	char a;
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	printf("inter character\n");
	scanf(" %c",&a);
	if (((a>'a')&&(a<'z'))||((a>'A')&&(a<'A')))
	{
		printf("%c is alphabet	",a);
	}
	else if (a<0)
	{
		printf("%c isn't alphabet",a);
	}
}



