
#include "stdio.h"
void main ()
{
	char a;
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	printf("inter number\n");
	scanf("%c",&a);
	if (((a>'a')&&(a<'z'))||((a>'A')&&(a<Z)))
	{
		printf("%C is ALP	",a);
	}
	else if (a<0)
	{
		printf("%.2f is negative",a);
	}
	else
	{
		printf("you entered zero");
	}

}



