
#include "stdio.h"
void main ()
{
	float a;
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	printf("inter number\n");
	scanf("%f",&a);
	if (a>0)
	{
		printf("%f is positive",a);
	}
	else if (a<0)
	{
		printf("%f is negative",a);
	}
	else
	{
		printf("you entered zero");
	}

}



