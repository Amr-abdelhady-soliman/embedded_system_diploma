#include "stdio.h"
struct Sstudent
{
	char name[50];
	int roll;
	float marks;
};
struct Sstudent mohamed;

void main ()
{
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);

	printf("enter name : ");
	scanf("%s", &mohamed.name);
	printf("/n %s %d %f", mohamed.name,mohamed.roll,mohamed.marks );
}
